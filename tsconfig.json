{
  "compilerOptions": {
    "target": "ES5",
    "outDir": "./dist/out/",
    "rootDir": "./src/",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "incremental": false,
    "declaration": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src"],
  "exclude": ["node_modules"]
}
